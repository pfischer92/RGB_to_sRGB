<Window x:Class="CornellBoxWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CornellBoxWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="frames" HorizontalAlignment="Left" VerticalAlignment="Top" />
        <Image x:Name="img" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,25,0,0" />
        <Image x:Name="img2" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,100,0,0" />
        <Image x:Name="anim" Stretch="None" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,200,0,0" />
    </Grid>
</Window>
